  "private": true,
  "dependencies": {
    "axios": "^0.21.0",
    "cors": "^2.8.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "twitter": "1.3.0"
  },
  "scripts": {
